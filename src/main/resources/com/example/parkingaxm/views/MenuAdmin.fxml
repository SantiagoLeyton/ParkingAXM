<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.*?>

<VBox spacing="15" alignment="CENTER" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.parkingaxm.controllers.MenuController">

    <Label text="Menú Administrador" />

    <Button text="Registrar Vehículo" fx:id="btnRegistrar" onAction="#onRegistrarVehiculo"/>
    <Button text="Listar Vehículos" fx:id="btnListar" onAction="#onListarVehiculos"/>
    <Button text="Cobro" fx:id="btnCobro" onAction="#onCobro"/>
    <Button text="Estadísticas" fx:id="btnEstadisticas" onAction="#onEstadisticas"/>
    <Button text="Crear Usuario" fx:id="btnCrearUsuario" onAction="#onCrearUsuario"/>
    <Button text="Cerrar Sesión" fx:id="btnSalir" onAction="#onSalir"/>

</VBox>
